{
    "name": "webdriverio_e2e",
    "version": "1.0.0",
    "description": "Boilerplate project using webdriverio with mocha and BDD framework",
    "main": "index.js",
    "scripts": {
        "start": "ts-node runner.ts",
        "test": "wdio run ./src/web/config/wdio.conf.ts",
        "pretest:docker": "docker-compose up -d",
        "test:docker": "wdio run ./src/web/config/wdio.conf.docker.ts",
        "posttest:docker": "docker-compose down",
        "report:mocha": "marge ./reports/mocha/wdio-ma-merged.json --reportTitle 'WebAppMochaReport' && move mochawesome-report.html ./mochawesome-report",
        "generateMochawesome": "marge ./reports/mocha/wdio-ma-merged.json --reportTitle 'My Project Results'",
        "report:mocha:ci": "marge ./reports/mocha/wdio-ma-merged.json",
        "test:e2e": "wdio run ./src/web/config/wdio.conf.e2e.ts",
        "pretest:e2e:docker": "docker-compose up -d",
        "test:e2e:docker": "wdio run ./src/web/config/wdio.conf.e2e.docker.ts",
        "posttest:e2e:docker": "docker-compose down",
        "report:cucumber": "ts-node src/web/generator/index.ts",
        "mailCucumberReport": "npm run report:cucumber && ts-node ./src/web/utils/mailer.ts",
        "test:api": "mocha --timeout 200000 --reporter mochawesome --reporter-options reportDir=reports/api -r tsconfig-paths/register -r ts-node/register src/api/test/*.ts",
        "test-mobile": "wdio run ./src/mobile/wdio.conf.ts",
        "report-mobile": "marge ./reports/appium/wdio-ma-merged.json --reportTitle 'AppiumReport' --reportDir=reports/mobile && move .\\reports\\mobile.html .\\reports\\mobile",
        "list-driver": "appium driver list",
        "install-driver-android": "appium driver install uiautomator2",
        "install-driver-ios": "appium driver install xcuitest",
        "android-doctor": "appium-doctor",
        "wdio": "wdio run test/wdio.conf.ts",
        "test:api:mocha": "mocha --config src/api/config/.mocharc.json"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/TinNguyenWav/WebdriverIOE2E#readme"
    },
    "author": "Tin Nguyen",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/TinNguyenWav/WebdriverIOE2E/issues"
    },
    "homepage": "TBD",
    "devDependencies": {
        "@faker-js/faker": "^6.0.0-alpha.6",
        "@types/chai": "^4.3.0",
        "@types/nodemailer": "^6.4.4",
        "@types/supertest": "^2.0.11",
        "@wdio/appium-service": "^7.16.14",
        "@wdio/cli": "^7.16.15",
        "@wdio/cucumber-framework": "^7.16.14",
        "@wdio/junit-reporter": "^7.16.15",
        "@wdio/local-runner": "^7.16.15",
        "@wdio/mocha-framework": "^7.16.15",
        "@wdio/selenium-standalone-service": "^7.16.14",
        "@wdio/spec-reporter": "^7.16.14",
        "appium": "^1.22.2",
        "appium-doctor": "^1.16.0",
        "chai": "^4.3.6",
        "cucumber-html-reporter": "^5.5.0",
        "dotenv": "^16.0.0",
        "mochawesome": "^7.0.1",
        "mochawesome-report-generator": "^6.0.1",
        "nodemailer": "^6.7.2",
        "supertest": "^6.2.2",
        "ts-node": "^10.5.0",
        "tsconfig-paths": "^3.12.0",
        "typescript": "^4.5.5",
        "wdio-chromedriver-service": "^7.2.8",
        "wdio-cucumberjs-json-reporter": "^4.4.0",
        "wdio-docker-service": "^3.2.0",
        "wdio-mochawesome-reporter": "^4.0.0",
        "zip-local": "^0.3.5"
    },
    "dependencies": {
        "enquirer": "^2.3.6",
        "node-html-to-image": "^3.2.0",
        "uuid": "^8.3.2"
    }
}